[config.toml]

concurrent = 1
check_interval = 0

[session_server]
  session_timeout = 1800
  listen_address = "[::]:8093"   
  advertise_address = "runner-host-name.tld:8093"

[[runners]]
  name = "docker-runner"
  url = "http://xxx.xx.xx.xx/"
  token = "<runner-token>"
  executor = "docker"
  [runners.custom_build_dir]
  [runners.cache]
    [runners.cache.s3]
    [runners.cache.gcs]
    [runners.cache.azure]
  [runners.docker]
    pull_policy = "if-not-present"
    tls_verify = false
    image = "alpine:latest"
    privileged = false
    disable_entrypoint_overwrite = false
    oom_kill_disable = false
    disable_cache = false
    volumes = ["/cache"]
    shm_size = 0